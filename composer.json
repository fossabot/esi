{
    "name": "agrimes94/esi",
    "description": "PHP Http Client for ESI (EVE Swagger Interface)",
    "type": "library",
    "keywords": [
        "http client",
        "eve online",
        "esi"
    ],
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Anthony Grimes",
            "email": "contact@anthonygrimes.co.uk",
            "homepage": "https://anthonygrimes.co.uk/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/aGrimes94/esi/issues",
        "source": "https://github.com/aGrimes94/esi"
    },
    "require": {
        "php": ">=7.1",
        "php-http/client-common": "^1.7",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "php-http/mock-client": "^1.1",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.0",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "AGrimes94\\Esi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AGrimes94\\Esi\\Tests\\": "tests/"
        }
    }
}
